  "main": "index.js",
  "scripts": {
    "start:node": "CERAMIC_ENABLE_EXPERIMENTAL_COMPOSE_DB='true' pnpm dlx @ceramicnetwork/cli daemon",
    "start:graphql": "composedb graphql:server --ceramic-url=http://localhost:7007 --graphiql --port=5001 runtime-composite.json",
    "echo:did": "composedb did:from-private-key $(cat local.key)",
    "compile:composite": "composedb composite:compile --did-private-key=$(cat local.key) my-first-composite.json runtime-composite.json",
    "deploy:composite": "composedb composite:deploy --did-private-key=$(cat local.key) my-first-composite.json --ceramic-url=http://localhost:7007",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
